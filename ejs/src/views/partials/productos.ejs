<div>
    <table>
        <tr>
            <th>Nombre</th>
            <th>Precio</th>
            <th>Imagen</th>
        </tr>
            <% if(productos.length > 0) { %>
                <% productos.forEach(function(producto) { %>
                    <tr>
                        <td> <%= producto.title %> </td>
                        <td><%= producto.price %> </td>
                        <td><img src="<%= producto.thumbnail %>" alt=""></td>  
                    </tr>
                <% }) %>
            <% } else { %>
                    <p>No hay productos cargados</p>
            <% } %>
            
    </table>
</div>