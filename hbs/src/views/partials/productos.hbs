<div>
    <table>
        <tr>
            <th>Nombre</th>
            <th>Precio</th>
            <th>Imagen</th>
        </tr>
        {{#if exist}}
            {{#each productos}}
                <tr>
                    <td>{{this.title}} </td>
                    <td>{{this.price}} </td>
                    <td>{{this.thumbnail}} </td>  
                </tr>
            {{/each}}
        {{/if}}
    </table>
</div>
