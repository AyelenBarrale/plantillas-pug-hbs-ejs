<div>
    <table class="tabla-container">
        <tr>
            <th>Nombre</th>
            <th>Precio</th>
            <th>Imagen</th>
        </tr>
        {{#if productos}}
            {{#each productos}}
                <tr>
                    <td>{{this.title}} </td>
                    <td>{{this.price}} </td>
                    <td><img src="{{this.thumbnail}}" alt=""></td> 
                    
                </tr>
            {{/each}}
        {{else}}
            <p>No hay productos cargados</p>
        {{/if}}
    </table>
</div>
